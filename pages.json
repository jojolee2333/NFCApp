    		"borderStyle": "black",
    		"backgroundColor": "#ffffff",
    		"height": "50px",
    		"fontSize": "10px",
    		"iconWidth": "24px",
    		"spacing": "3px",
        	"iconfontSrc":"static/iconfont.ttf", // app tabbar 字体.ttf文件路径 app 3.4.4+
